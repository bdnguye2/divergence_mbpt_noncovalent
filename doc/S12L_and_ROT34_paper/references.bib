@article{b415208e,
author ="H{\"a}ttig, Christof",
title  ="Optimization of auxiliary basis sets for RI-MP2 and RI-CC2 calculations: Core-valence and quintuple-$\zeta$ basis sets for H to Ar and QZVPP basis sets for Li to Kr",
journal  ="Phys. Chem. Chem. Phys.",
year  ="2005",
