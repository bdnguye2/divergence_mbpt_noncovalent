$rirpa
  npoints 100
  nohxx
  mklcpus 10
$title
$operating system unix
$symmetry c1
$redundant    file=coord
$user-defined bonds    file=coord
$coord    file=coord
$optimize
 internal   on
 redundant  on
 cartesian  off
 global     off
 basis      off
$atoms
c  1-9,11-20,22-41,43-52,54-64                                                 \
   basis =c cc-pVQZ                                                            \
   cbas  =c cc-pVQZ                                                            \
   jbas  =c cc-pVQZ
n  10,21,42,53                                                                 \
   basis =n cc-pVQZ                                                            \
   cbas  =n cc-pVQZ                                                            \
   jbas  =n cc-pVQZ
h  65-112                                                                      \
   basis =h cc-pVQZ                                                            \
   cbas  =h cc-pVQZ                                                            \
   jbas  =h cc-pVQZ
cl 113-116                                                                     \
   basis =cl cc-pVQZ                                                           \
   cbas  =cl cc-pVQZ                                                           \
   jbas  =cl cc-pVQZ
$basis    file=basis
$rundimensions
   dim(fock,dens)=20864376
   natoms=116
   nshell=1508
   nbf(CAO)=6456
   dim(trafo[SAO<-->AO/CAO])=9044
   rhfshells=1
   nbf(AO)=5196
$scfmo   file=mos
$scfiterlimit 300
$thize     0.10000000E-04
$thime        5
$scfdump
$scfintunit
 unit=30       size=0        file=twoint
$scfdiis
$maxcor  14000
$scforbitalshift  automatic=.1
$drvopt
   cartesian  on
   basis      off
   global     off
   hessian    on
   dipole     on
   nuclear polarizability
$interconversion  off
   qconv=1.d-7
   maxiter=25
$coordinateupdate
   dqmax=0.3
   interpolate  on
   statistics    5
$forceupdate
   ahlrichs numgeo=0  mingeo=3 maxgeo=4 modus=<g|dq> dynamic fail=0.3
   threig=0.005  reseig=0.005  thrbig=3.0  scale=1.00  damping=0.0
$forceinit on
   diag=default
$energy    file=energy
$grad    file=gradient
$forceapprox    file=forceapprox
$dft
   functional tpss
   gridsize   m5
$scfconv 9
$scfdamp   start=0.700  step=0.050  min=0.050
$ricore    12000
$rij
$jbas    file=auxbasis
$frag    file=coord
$denconv     0.10000000E-06
$cbas    file=auxbasis
$lock off
$closed shells
 a       1-252                                  ( 2 )
$freeze
 implicit core=   84 virt=    0
$actual step      rirpa
$end
